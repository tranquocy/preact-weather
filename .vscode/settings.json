{
  "editor.quickSuggestions": {
    "strings": true
  },
  "css.validate": false,
  "stylelint.enable": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "[markdown]": {
    "editor.formatOnSave": true
  },
  "[html]": {
    "editor.formatOnSave": true
  },
  "[css]": {
    "editor.formatOnSave": true
  },
  "files.associations": {
    ".stylelintrc": "jsonc",
    ".eslintrc": "jsonc"
  },
}
